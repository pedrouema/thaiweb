{"ast":null,"code":"import Chart from 'chart.js/auto';\nexport default {\n  name: 'AnalisesAlunos',\n  data() {\n    return {\n      URL: \"http://localhost:4000\"\n    };\n  },\n  methods: {},\n  mounted() {\n    const data = {\n      labels: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],\n      datasets: [{\n        label: 'Total de Alunos',\n        data: [25, 28, 35, 45, 40, 32, 42, 48],\n        fill: false,\n        borderColor: 'rgb(75, 192, 192)',\n        tension: 0.1\n      }]\n    };\n    const config = {\n      type: 'line',\n      data: data\n    };\n    const ctx = document.getElementById('myChartAlunos');\n    const myChartAlunos = new Chart(ctx, {\n      // The type of chart we want to create\n      type: 'line',\n      // The data for our dataset\n      data: {\n        labels: [\"January\", \"February\", \"March\", \"April\", \"May\", \"June\", \"July\"],\n        datasets: [{\n          label: \"My First dataset\",\n          backgroundColor: ['rgba(41, 128, 185,0.8)', 'rgba(41, 128, 185,0.8)', 'rgba(52, 73, 94,0.8)', 'rgba(44, 62, 80,0.8)', 'rgba(149, 165, 166,0.8)', 'rgba(127, 140, 141,0.8)'],\n          borderColor: ['rgba(41, 128, 185,1.0)', 'rgba(41, 128, 185,1.0)', 'rgba(52, 73, 94,1.0)', 'rgba(44, 62, 80,1.0)', 'rgba(149, 165, 166,1.0)', 'rgba(127, 140, 141,1.0)'],\n          data: [0, 10, 5, 2, 20, 30, 45]\n        }]\n      },\n      // Configuration options go here\n      options: {\n        animation: {\n          duration: 2000,\n          easing: 'easeOutBounce'\n        },\n        layout: {\n          padding: {\n            left: 20,\n            right: 20,\n            top: 20,\n            bottom: 20\n          }\n        },\n        legend: {\n          display: true,\n          position: 'bottom'\n        },\n        title: {\n          display: true,\n          text: 'Sales by Months',\n          fontSize: 20\n        },\n        tooltips: {\n          enabled: true,\n          intersect: true,\n          backgroundColor: 'rgba(41, 128, 185,0.8)'\n        },\n        scales: {\n          xAxes: [{\n            gridLines: {\n              display: false,\n              drawBorder: false\n            },\n            position: 'bottom'\n          }],\n          yAxes: [{\n            gridLines: {\n              display: false,\n              drawBorder: false\n            }\n          }]\n        }\n      }\n    });\n    myChartAlunos;\n  }\n};","map":{"version":3,"names":["Chart","name","data","URL","methods","mounted","labels","datasets","label","fill","borderColor","tension","config","type","ctx","document","getElementById","myChartAlunos","backgroundColor","options","animation","duration","easing","layout","padding","left","right","top","bottom","legend","display","position","title","text","fontSize","tooltips","enabled","intersect","scales","xAxes","gridLines","drawBorder","yAxes"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\analises\\analisesAlunos.vue"],"sourcesContent":["<template>\r\n    <h2>Total de alunos nos ultimos meses</h2>\r\n    <div style=\"position: relative; height: 45vh; width: 80vw\">\r\n        <canvas id=\"myChartAlunos\"></canvas>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Chart from 'chart.js/auto';\r\n\r\nexport default {\r\n    name: 'AnalisesAlunos',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n        }\r\n    },\r\n    methods: {\r\n\r\n    },\r\n    mounted() {\r\n        const data = {\r\n            labels: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],\r\n            datasets: [{\r\n                label: 'Total de Alunos',\r\n                data: [25, 28, 35, 45, 40, 32, 42, 48, ],\r\n                fill: false,\r\n                borderColor: 'rgb(75, 192, 192)',\r\n                tension: 0.1\r\n            }]\r\n        };\r\n\r\n        const config = {\r\n            type: 'line',\r\n            data: data,\r\n        };\r\n\r\n        const ctx = document.getElementById('myChartAlunos');\r\n        const myChartAlunos = new Chart(ctx, {\r\n    // The type of chart we want to create\r\n    type: 'line',\r\n    // The data for our dataset\r\n    data: {\r\n        labels: [\"January\", \"February\", \"March\", \"April\", \"May\", \"June\", \"July\"],\r\n        datasets: [{\r\n            label: \"My First dataset\",\r\n            backgroundColor: [\r\n                'rgba(41, 128, 185,0.8)',\r\n                'rgba(41, 128, 185,0.8)',\r\n                'rgba(52, 73, 94,0.8)',\r\n                'rgba(44, 62, 80,0.8)',\r\n                'rgba(149, 165, 166,0.8)',\r\n                'rgba(127, 140, 141,0.8)'\r\n            ],\r\n            borderColor: [\r\n                'rgba(41, 128, 185,1.0)',\r\n                'rgba(41, 128, 185,1.0)',\r\n                'rgba(52, 73, 94,1.0)',\r\n                'rgba(44, 62, 80,1.0)',\r\n                'rgba(149, 165, 166,1.0)',\r\n                'rgba(127, 140, 141,1.0)'\r\n            ],\r\n            data: [0, 10, 5, 2, 20, 30, 45],\r\n        }]\r\n    },\r\n    // Configuration options go here\r\n    options: {\r\n        animation : {\r\n            duration : 2000,\r\n            easing : 'easeOutBounce'\r\n        },\r\n        layout : {\r\n            padding : {\r\n                left : 20,\r\n                right : 20,\r\n                top : 20,\r\n                bottom : 20\r\n            }\r\n        },\r\n        legend : {\r\n            display : true,\r\n            position : 'bottom'\r\n        },\r\n        title : {\r\n            display : true,\r\n            text : 'Sales by Months',\r\n            fontSize : 20\r\n        },\r\n        tooltips : {\r\n            enabled : true,\r\n            intersect : true,\r\n            backgroundColor : 'rgba(41, 128, 185,0.8)'\r\n        },\r\n        scales : {\r\n            xAxes : [{\r\n                gridLines : {\r\n                    display : false,\r\n                    drawBorder : false\r\n                },\r\n                position : 'bottom'\r\n            }],\r\n            yAxes : [{\r\n\r\n                gridLines : {\r\n                    display : false,\r\n                    drawBorder : false\r\n                }\r\n            }]\r\n        }\r\n    }\r\n});\r\n        myChartAlunos;\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n\r\n"],"mappings":"AAQA,OAAOA,KAAI,MAAO,eAAe;AAEjC,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE;IACT;EACJ,CAAC;EACDC,OAAO,EAAE,CAET,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,MAAMH,IAAG,GAAI;MACTI,MAAM,EAAE,CAAC,SAAS,EAAE,WAAW,EAAE,OAAO,EAAE,OAAO,EAAE,MAAM,EAAE,OAAO,EAAE,OAAO,EAAE,QAAQ,EAAE,UAAU,EAAE,SAAS,EAAE,UAAU,EAAE,UAAU,CAAC;MACrIC,QAAQ,EAAE,CAAC;QACPC,KAAK,EAAE,iBAAiB;QACxBN,IAAI,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE,CAAG;QACxCO,IAAI,EAAE,KAAK;QACXC,WAAW,EAAE,mBAAmB;QAChCC,OAAO,EAAE;MACb,CAAC;IACL,CAAC;IAED,MAAMC,MAAK,GAAI;MACXC,IAAI,EAAE,MAAM;MACZX,IAAI,EAAEA;IACV,CAAC;IAED,MAAMY,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,eAAe,CAAC;IACpD,MAAMC,aAAY,GAAI,IAAIjB,KAAK,CAACc,GAAG,EAAE;MACzC;MACAD,IAAI,EAAE,MAAM;MACZ;MACAX,IAAI,EAAE;QACFI,MAAM,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,OAAO,EAAE,OAAO,EAAE,KAAK,EAAE,MAAM,EAAE,MAAM,CAAC;QACxEC,QAAQ,EAAE,CAAC;UACPC,KAAK,EAAE,kBAAkB;UACzBU,eAAe,EAAE,CACb,wBAAwB,EACxB,wBAAwB,EACxB,sBAAsB,EACtB,sBAAsB,EACtB,yBAAyB,EACzB,yBAAwB,CAC3B;UACDR,WAAW,EAAE,CACT,wBAAwB,EACxB,wBAAwB,EACxB,sBAAsB,EACtB,sBAAsB,EACtB,yBAAyB,EACzB,yBAAwB,CAC3B;UACDR,IAAI,EAAE,CAAC,CAAC,EAAE,EAAE,EAAE,CAAC,EAAE,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,EAAE;QAClC,CAAC;MACL,CAAC;MACD;MACAiB,OAAO,EAAE;QACLC,SAAQ,EAAI;UACRC,QAAO,EAAI,IAAI;UACfC,MAAK,EAAI;QACb,CAAC;QACDC,MAAK,EAAI;UACLC,OAAM,EAAI;YACNC,IAAG,EAAI,EAAE;YACTC,KAAI,EAAI,EAAE;YACVC,GAAE,EAAI,EAAE;YACRC,MAAK,EAAI;UACb;QACJ,CAAC;QACD