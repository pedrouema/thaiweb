v class=\"col-12\">\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"buscarRecebidas()\">\r\n                <i class=\"fa fa-search\"></i> BUSCAR</button>\r\n        </div>\r\n    </form>\r\n    <br>\r\n    <div class=\"recebidas-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Nome</th>\r\n                    <th scope=\"col\">CPF</th>\r\n                    <th scope=\"col\">Data Recebido</th>\r\n                    <th scope=\"col\">Valor Recebido</th>\r\n                    <th scope=\"col\">Ação</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"recebida in recebidas\" :key=\"recebida.id_mensalidade\">\r\n                    <th scope=\"row\">{{ recebida.nome_aluno }}</th>\r\n                    <td>{{ recebida.cpf_aluno }}</td>\r\n                    <td>{{ recebida.data_recebido }}</td>\r\n                    <td>{{ recebida.valor_recebimento }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"showAlertVoltar(despesa.id_despesa)\">\r\n                            <i class=\"fa fa-reply-all\"> </i>\r\n                             Voltar (Não Foi Recebido)\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: \"VerMensalidadesRecebidas\",\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            recebidas: [],\r\n            dataIni: '',\r\n            dataFim: '',\r\n        }\r\n    },\r\n    methods: {\r\n        getAllRecebidas() {\r\n            axios.get(`${this.URL}/getallrecebidas`).then(response => {\r\n                this.recebidas = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        buscarRecebidas() {\r\n            if(this.dataIni == '' && this.dataFim == ''){\r\n                this.getAllRecebidas()\r\n            }\r\n            else{\r\n                this.retornaRecebidasEntreDatas()\r\n                console.log('Passou por data despesas quitadas');\r\n                this.limpaDados()\r\n            }\r\n        },\r\n        retornaRecebidasEntreDatas(){\r\n            axios.get(`${this.URL}/recebidasentredatas/${this.dataIni}/${this.dataFim}`).then(response => {\r\n                this.recebidas = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAgDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,0BAA0B;EAChCC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,eAAeA,CAAA,EAAG;MACdR,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,kBAAiB,CAAC,CAACO,IAAI,CAACC,QAAO,IAAK;QACtD,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACT,IAAG;MACjC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDG,eAAeA,CAAA,EAAG;MACd,IAAG,IAAI,CAACX,OAAM,IAAK,EAAC,IAAK,IAAI,CAACC,OAAM,IAAK,EAAE,EAAC;QACxC,IAAI,CAACE,eAAe,EAAC;MACzB,OACI;QACA,IAAI,CAACS,0BAA0B,EAAC;QAChCH,OAAO,CAACC,GAAG,CAAC,mCAAmC,CAAC;QAChD,IAAI,CAACG,UAAU,EAAC;MACpB;IACJ,CAAC;IACDD,0BAA0BA,CAAA,EAAE;MACxBjB,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,wBAAuB,IAAI,CAACE,OAAQ,IAAG,IAAI,CAACC,OAAQ,EAAC,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC1F,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACT,IAAG;MACjC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;IACL;EACJ;AAEJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}