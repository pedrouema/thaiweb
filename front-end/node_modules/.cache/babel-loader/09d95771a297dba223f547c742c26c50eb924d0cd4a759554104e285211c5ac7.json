{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LancarDespesa',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      despesas: [],\n      nome: '',\n      valor: '',\n      dataVenc: '',\n      despesaId: 0,\n      editar: false\n    };\n  },\n  methods: {\n    getAllDespesas() {\n      axios.get(`${this.URL}/despesas`).then(response => {\n        this.despesas = response.data;\n        console.log(this.despesas);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    createDespesa() {\n      const despesa = {\n        nome_despesa: this.nome,\n        valor_despesa: this.valor,\n        vencimento_despesa: this.dataVenc\n      };\n      console.log(despesa);\n      axios.post(`${this.URL}/despesas`, despesa).then(response => {\n        console.log(response);\n        this.getAllDespesas();\n      });\n      this.limpaDadosFormulario();\n    },\n    salvarDados() {\n      const despesa = {\n        nome_despesa: this.nome,\n        valor_despesa: this.valor,\n        vencimento_despesa: this.dataVenc\n      };\n      console.log(despesa);\n      axios.put(`${this.URL}/despesas/${this.despesaId}`, despesa).then(response => {\n        console.log(response);\n        this.getAllDespesas();\n      });\n      this.limpaDadosFormulario();\n    },\n    quitarDespesa(id) {\n      this.editar = true;\n      const despesa = {\n        quitada_despesa: true\n      };\n      console.log(this.despesa);\n      console.log(\"id=\" + id);\n      axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\n        console.log(response);\n        this.getAllDespesas();\n      });\n      this.limpaDadosFormulario();\n    },\n    carregarDadosDespesa(id) {\n      this.editar = true;\n      axios.get(`${this.URL}/despesas/getone/` + id).then(response => {\n        const despesa = response.data[0];\n        this.despesaId = despesa.id_despesa;\n        this.nome = despesa.nome_despesa;\n        this.valor = despesa.valor_despesa;\n        this.dataVenc = despesa.vencimento_despesa_formatada;\n        console.log(response.data[0]);\n      });\n    },\n    limpaDadosFormulario() {\n      this.nome = '';\n      this.valor = 0, this.dataVenc = '';\n      this.despesaId = 0;\n      this.editar = false;\n    }\n  },\n  mounted() {\n    this.getAllDespesas();\n  }\n};","map":{"version":3,"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,IAAI,GAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,CAAC;MACZC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,cAAc,GAAG;MACbX,KAAK,CAACY,GAAG,CAAE,GAAE,IAAI,CAACT,GAAI,WAAU,CAAC,CAACU,IAAI,CAACC,QAAO,IAAK;QAC/C,IAAI,CAACV,QAAO,GAAIU,QAAQ,CAACZ,IAAG;QAC5Ba,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,QAAQ,CAAC;MAC9B,CAAC,EACAa,KAAK,CAACC,KAAI,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDC,aAAa,GAAE;MACX,MAAMC,OAAM,GAAI;QACZC,YAAY,EAAE,IAAI,CAAChB,IAAI;QACvBiB,aAAa,EAAE,IAAI,CAAChB,KAAK;QACzBiB,kBAAkB,EAAE,IAAI,CAAChB;MAC7B;MACAQ,OAAO,CAACC,GAAG,CAACI,OAAO,CAAC;MACpBpB,KAAK,CAACwB,IAAI,CAAE,GAAE,IAAI,CAACrB,GAAI,WAAU,EAAEiB,OAAO,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QACzDC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACH,cAAc,EAAE;MAEzB,CAAC;MACD,IAAI,CAACc,oBAAoB,EAAC;IAC9B,CAAC;IACDC,WAAW,GAAE;MACT,MAAMN,OAAM,GAAI;QACZC,YAAY,EAAE,IAAI,CAAChB,IAAI;QACvBiB,aAAa,EAAE,IAAI,CAAChB,KAAK;QACzBiB,kBAAkB,EAAE,IAAI,CAAChB;MAC7B;MACAQ,OAAO,CAACC,GAAG,CAACI,OAAO,CAAC;MACpBpB,KAAK,CAAC2B,GAAG,CAAE,GAAE,IAAI,CAACxB,GAAI,aAAY,IAAI,CAACK,SAAU,EAAC,EAAEY,OAAO,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QAC1EC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACH,cAAc,EAAE;MACzB,CAAC,CAAC;MACF,IAAI,CAACc,oBAAoB,EAAE;IAC/B,CAAC;IACDG,aAAa,CAACC,EAAE,EAAC;MACb,IAAI,CAACpB,MAAK,GAAI,IAAG;MACjB,MAAMW,OAAM,GAAI;QACZU,eAAe,EAAE;MACrB;MACAf,OAAO,CAACC,GAAG,CAAC,IAAI,CAACI,OAAO,CAAC;MACzBL,OAAO,CAACC,GAAG,CAAC,KAAK,GAACa,EAAE,CAAC;MACrB7B,KAAK,CAAC2B,GAAG,CAAE,GAAE,IAAI,CAACxB,GAAI,oBAAmB0B,EAAG,EAAC,EAAET,OAAO,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QACrEC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACH,cAAc,EAAE;MACzB,CAAC,CAAC;MACF,IAAI,CAACc,oBAAoB,EAAE;IAC/B,CAAC;IACDM,oBAAoB,CAACF,EAAE,EAAC;MACpB,IAAI,CAACpB,MAAK,GAAI,IAAI;MAClBT,KAAK,CAACY,GAAG,CAAE,GAAE,IAAI,CAACT,GAAI,mBAAkB,GAAC0B,EAAE,CAAC,CAAChB,IAAI,CAACC,QAAO,IAAK;QAC1D,MAAMM,OAAM,GAAIN,QAAQ,CAACZ,IAAI,CAAC,CAAC;QAC/B,IAAI,CAACM,SAAQ,GAAIY,OAAO,CAACY,UAAS;QAClC,IAAI,CAAC3B,IAAG,GAAIe,OAAO,CAACC,YAAW;QAC/B,IAAI,CAACf,KAAI,GAAIc,OAAO,CAACE,aAAY;QACjC,IAAI,CAACf,QAAO,GAAIa,OAAO,CAACa,4BAA2B;QACnDlB,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACZ,IAAI,CAAC,CAAC,CAAC,CAAC;MACjC,CAAC;IACL,CAAC;IACDuB,oBAAoB,GAAE;MAClB,IAAI,CAACpB,IAAG,GAAI,EAAE;MACd,IAAI,CAACC,KAAI,GAAI,CAAC,EACd,IAAI,CAACC,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,SAAQ,GAAI,CAAC;MAClB,IAAI,CAACC,MAAK,GAAI,KAAK;IACvB;EACJ,CAAC;EACDyB,OAAO,GAAG;IACN,IAAI,CAACvB,cAAc,EAAE;EACzB;AACJ","names":["axios","name","data","URL","despesas","nome","valor","dataVenc","despesaId","editar","methods","getAllDespesas","get","then","response","console","log","catch","error","createDespesa","despesa","nome_despesa","valor_despesa","vencimento_despesa","post","limpaDadosFormulario","salvarDados","put","quitarDespesa","id","quitada_despesa","carregarDadosDespesa","id_despesa","vencimento_despesa_formatada","mounted"],"sourceRoot":"","sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\financeiro\\lancarDespesa.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h3>Gerenciamento de Despesas</h3>\r\n        <br/>\r\n        <form class=\"row g-3\">\r\n            <div class=\"col-md-6\">\r\n                <label for=\"inputEmail4\" class=\"form-label\">Nome da Despesa</label>\r\n                <input type=\"text\" class=\"form-control\" placeholder=\"ex: Energia\" v-model=\"nome\">\r\n            </div>\r\n            <div class=\"col-md-2\">\r\n                <label for=\"inputZip\" class=\"form-label\">Valor da Despesa</label>\r\n                <input type=\"text\" class=\"form-control\" placeholder=\"ex: 185.50\" v-model=\"valor\">\r\n            </div>\r\n            <div class=\"col-md-2\">\r\n                <label for=\"inputZip\" class=\"form-label\">Data de Vencimento</label >\r\n                <input type=\"date\" class=\"form-control\" v-model=\"dataVenc\">\r\n            </div>\r\n            <div class=\"col-12\">\r\n                <button type=\"button\" class=\"btn btn-primary\" @click=\"createDespesa()\" v-show=\"!editar\">+ SALVAR</button>\r\n                <button type=\"button\" class=\"btn btn-primary\" @click=\"salvarDados()\" v-show=\"editar\">+ EDITAR</button>\r\n                <button  type=\"button\" class=\"btn btn-danger\" @click=\"limpaDadosFormulario()\" >CANCELAR</button>\r\n            </div>\r\n        </form>\r\n    </div>\r\n    <br/>\r\n    <div class=\"alunos-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Despesa</th>\r\n                    <th scope=\"col\">Valor</th>\r\n                    <th scope=\"col\">Data Vencimento</th>\r\n                    <th scope=\"col\">Ações</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"despesa in despesas\" :key=\"despesa.id_despesa\">\r\n                    <th scope=\"row\">{{ despesa.nome_despesa }}</th>\r\n                    <td>R$ {{ despesa.valor_despesa.toFixed(2) }}</td>\r\n                    <td>{{ despesa.vencimento_despesa }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-success\" @click=\"carregarDadosDespesa(despesa.id_despesa)\">\r\n                            <i class=\"fa fa-pencil\"> </i>\r\n                             Editar\r\n                        </button>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"quitarDespesa(despesa.id_despesa)\">\r\n                            <i class=\"fa fa-check\"> </i>\r\n                             Quitar\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'LancarDespesa',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            despesas: [],\r\n            nome: '',\r\n            valor: '',\r\n            dataVenc: '',\r\n            despesaId: 0,\r\n            editar: false\r\n        }\r\n    },\r\n    methods: {\r\n        getAllDespesas() {\r\n            axios.get(`${this.URL}/despesas`).then(response => {\r\n                this.despesas = response.data\r\n                console.log(this.despesas);\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        createDespesa(){\r\n            const despesa = {\r\n                nome_despesa: this.nome,\r\n                valor_despesa: this.valor,\r\n                vencimento_despesa: this.dataVenc,\r\n            }\r\n            console.log(despesa);\r\n            axios.post(`${this.URL}/despesas`, despesa).then(response => {\r\n                console.log(response);\r\n                this.getAllDespesas();\r\n                \r\n            })\r\n            this.limpaDadosFormulario()\r\n        },\r\n        salvarDados(){\r\n            const despesa = {\r\n                nome_despesa: this.nome,\r\n                valor_despesa: this.valor,\r\n                vencimento_despesa: this.dataVenc,\r\n            }\r\n            console.log(despesa);\r\n            axios.put(`${this.URL}/despesas/${this.despesaId}`, despesa).then(response => {\r\n                console.log(response);\r\n                this.getAllDespesas();\r\n            });\r\n            this.limpaDadosFormulario();\r\n        },\r\n        quitarDespesa(id){\r\n            this.editar = true\r\n            const despesa = {\r\n                quitada_despesa: true,\r\n            }\r\n            console.log(this.despesa);\r\n            console.log(\"id=\"+id);\r\n            axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\r\n                console.log(response);\r\n                this.getAllDespesas();\r\n            });\r\n            this.limpaDadosFormulario();\r\n        },\r\n        carregarDadosDespesa(id){\r\n            this.editar = true;\r\n            axios.get(`${this.URL}/despesas/getone/`+id).then(response => {\r\n                const despesa = response.data[0]\r\n                this.despesaId = despesa.id_despesa\r\n                this.nome = despesa.nome_despesa\r\n                this.valor = despesa.valor_despesa\r\n                this.dataVenc = despesa.vencimento_despesa_formatada\r\n                console.log(response.data[0]);\r\n            })\r\n        },\r\n        limpaDadosFormulario(){\r\n            this.nome = '';\r\n            this.valor = 0,\r\n            this.dataVenc = '';\r\n            this.despesaId = 0;\r\n            this.editar = false;\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getAllDespesas();\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}