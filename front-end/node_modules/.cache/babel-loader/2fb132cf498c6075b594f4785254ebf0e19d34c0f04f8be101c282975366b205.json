{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Chart from 'chart.js/auto';\nexport default {\n  name: 'AnalisesAlunos',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      meses: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],\n      mesesUsar: [],\n      qtdeAlunos: []\n    };\n  },\n  methods: {\n    filtraMeses() {\n      let mes = this.mesAtual - 1;\n      for (let i = 0; i < 7; i++) {\n        if (mes == -1) mes = this.meses.length - 1;\n        this.mesesUsar.push(this.meses[mes]);\n        mes--;\n      }\n      let aux = [];\n      for (let i = this.mesesUsar.length - 1; i >= 0; i--) aux.push(this.mesesUsar[i]);\n      this.mesesUsar = aux;\n    },\n    pegarDataAtual() {\n      const data = new Date();\n      const dia = String(data.getDate()).padStart(2, '0');\n      const mes = String(data.getMonth() + 1).padStart(2, '0');\n      const ano = String(data.getFullYear());\n      const atual = `${dia}/${mes}/${ano}`;\n      this.mesAtual = mes;\n      this.diaAtual = dia;\n      this.anoAtual = ano;\n      this.dataAtual = atual;\n      this.mesAno = mes + '-' + ano;\n      this.diaDaSemana = data.toLocaleString('pt-BR', {\n        weekday: 'long'\n      });\n    }\n  },\n  mounted() {\n    this.pegarDataAtual();\n    this.filtraMeses();\n    const data = {\n      labels: this.meses,\n      datasets: [{\n        label: 'Total de Alunos',\n        data: this.qtdeAlunos,\n        fill: false,\n        borderColor: 'rgb(75, 192, 192)',\n        tension: 0.3\n      }]\n    };\n    const config = {\n      type: 'line',\n      data: data,\n      options: {\n        layout: {\n          padding: {\n            left: 20,\n            right: 20,\n            top: 20,\n            bottom: 20\n          }\n        },\n        legend: {\n          display: false,\n          position: 'bottom'\n        },\n        title: {\n          display: true,\n          text: 'Sales by Months',\n          fontSize: 20\n        },\n        scales: {\n          y: {\n            beginAtZero: true\n          }\n        }\n      }\n    };\n    const ctx = document.getElementById('myChartAlunos');\n    const myChartAlunos = new Chart(ctx, config);\n    myChartAlunos;\n  }\n};","map":{"version":3,"names":["Chart","name","data","URL","meses","mesesUsar","qtdeAlunos","methods","filtraMeses","mes","mesAtual","i","length","push","aux","pegarDataAtual","Date","dia","String","getDate","padStart","getMonth","ano","getFullYear","atual","diaAtual","anoAtual","dataAtual","mesAno","diaDaSemana","toLocaleString","weekday","mounted","labels","datasets","label","fill","borderColor","tension","config","type","options","layout","padding","left","right","top","bottom","legend","display","position","title","text","fontSize","scales","y","beginAtZero","ctx","document","getElementById","myChartAlunos"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\analises\\analisesAlunos.vue"],"sourcesContent":["<template>\r\n    <h2>Total de alunos nos ultimos meses</h2>\r\n    <div style=\"position: relative; height: 50vh; width: 80vw\">\r\n        <canvas id=\"myChartAlunos\"></canvas>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Chart from 'chart.js/auto';\r\n\r\nexport default {\r\n    name: 'AnalisesAlunos',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            meses: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],\r\n            mesesUsar: [],\r\n            qtdeAlunos: [],\r\n        }\r\n    },\r\n    methods: {\r\n        filtraMeses(){\r\n            let mes = this.mesAtual-1\r\n            for(let i = 0; i < 7; i++)\r\n            {\r\n                if(mes == -1)\r\n                    mes = this.meses.length - 1\r\n                this.mesesUsar.push(this.meses[mes])\r\n                mes--;\r\n            }\r\n            let aux = [];\r\n            for(let i = this.mesesUsar.length - 1; i >= 0; i--)\r\n                aux.push(this.mesesUsar[i])\r\n            this.mesesUsar = aux;\r\n        },\r\n        pegarDataAtual() {\r\n            const data = new Date()\r\n            const dia = String(data.getDate()).padStart(2, '0')\r\n            const mes = String(data.getMonth() + 1).padStart(2, '0')\r\n            const ano = String(data.getFullYear())\r\n            const atual = `${dia}/${mes}/${ano}`\r\n            this.mesAtual = mes\r\n            this.diaAtual = dia\r\n            this.anoAtual = ano\r\n            this.dataAtual = atual\r\n            this.mesAno = mes+'-'+ano\r\n            this.diaDaSemana = data.toLocaleString('pt-BR', {weekday: 'long'})\r\n        },\r\n    },\r\n    mounted() {\r\n        this.pegarDataAtual();\r\n        this.filtraMeses();\r\n\r\n        const data = {\r\n            labels: this.meses,\r\n            datasets: [{\r\n                label: 'Total de Alunos',\r\n                data: this.qtdeAlunos,\r\n                fill: false,\r\n                borderColor: 'rgb(75, 192, 192)',\r\n                tension: 0.3\r\n            }]\r\n        };\r\n\r\n        const config = {\r\n            type: 'line',\r\n            data: data,\r\n            options: {\r\n                layout : {\r\n                    padding : {\r\n                        left : 20,\r\n                        right : 20,\r\n                        top : 20,\r\n                        bottom : 20\r\n                    }\r\n                },\r\n                legend : {\r\n                    display : false,\r\n                    position : 'bottom'\r\n                },\r\n                title : {\r\n                    display : true,\r\n                    text : 'Sales by Months',\r\n                    fontSize : 20\r\n                },\r\n                scales: {\r\n                    y: {\r\n                        beginAtZero: true\r\n                    }\r\n                }\r\n            },\r\n        };\r\n\r\n        const ctx = document.getElementById('myChartAlunos');\r\n        const myChartAlunos = new Chart(ctx, config);\r\n        myChartAlunos;\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n\r\n"],"mappings":";AAQA,OAAOA,KAAI,MAAO,eAAe;AAEjC,eAAe;EACXC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,KAAK,EAAE,CAAC,SAAS,EAAE,WAAW,EAAE,OAAO,EAAE,OAAO,EAAE,MAAM,EAAE,OAAO,EAAE,OAAO,EAAE,QAAQ,EAAE,UAAU,EAAE,SAAS,EAAE,UAAU,EAAE,UAAU,CAAC;MACpIC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAE;MACT,IAAIC,GAAE,GAAI,IAAI,CAACC,QAAQ,GAAC;MACxB,KAAI,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,CAAC,EAAEA,CAAC,EAAE,EACzB;QACI,IAAGF,GAAE,IAAK,CAAC,CAAC,EACRA,GAAE,GAAI,IAAI,CAACL,KAAK,CAACQ,MAAK,GAAI;QAC9B,IAAI,CAACP,SAAS,CAACQ,IAAI,CAAC,IAAI,CAACT,KAAK,CAACK,GAAG,CAAC;QACnCA,GAAG,EAAE;MACT;MACA,IAAIK,GAAE,GAAI,EAAE;MACZ,KAAI,IAAIH,CAAA,GAAI,IAAI,CAACN,SAAS,CAACO,MAAK,GAAI,CAAC,EAAED,CAAA,IAAK,CAAC,EAAEA,CAAC,EAAE,EAC9CG,GAAG,CAACD,IAAI,CAAC,IAAI,CAACR,SAAS,CAACM,CAAC,CAAC;MAC9B,IAAI,CAACN,SAAQ,GAAIS,GAAG;IACxB,CAAC;IACDC,cAAcA,CAAA,EAAG;MACb,MAAMb,IAAG,GAAI,IAAIc,IAAI,EAAC;MACtB,MAAMC,GAAE,GAAIC,MAAM,CAAChB,IAAI,CAACiB,OAAO,EAAE,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG;MAClD,MAAMX,GAAE,GAAIS,MAAM,CAAChB,IAAI,CAACmB,QAAQ,EAAC,GAAI,CAAC,CAAC,CAACD,QAAQ,CAAC,CAAC,EAAE,GAAG;MACvD,MAAME,GAAE,GAAIJ,MAAM,CAAChB,IAAI,CAACqB,WAAW,EAAE;MACrC,MAAMC,KAAI,GAAK,GAAEP,GAAI,IAAGR,GAAI,IAAGa,GAAI;MACnC,IAAI,CAACZ,QAAO,GAAID,GAAE;MAClB,IAAI,CAACgB,QAAO,GAAIR,GAAE;MAClB,IAAI,CAACS,QAAO,GAAIJ,GAAE;MAClB,IAAI,CAACK,SAAQ,GAAIH,KAAI;MACrB,IAAI,CAACI,MAAK,GAAInB,GAAG,GAAC,GAAG,GAACa,GAAE;MACxB,IAAI,CAACO,WAAU,GAAI3B,IAAI,CAAC4B,cAAc,CAAC,OAAO,EAAE;QAACC,OAAO,EAAE;MAAM,CAAC;IACrE;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACjB,cAAc,EAAE;IACrB,IAAI,CAACP,WAAW,EAAE;IAElB,MAAMN,IAAG,GAAI;MACT+B,MAAM,EAAE,IAAI,CAAC7B,KAAK;M