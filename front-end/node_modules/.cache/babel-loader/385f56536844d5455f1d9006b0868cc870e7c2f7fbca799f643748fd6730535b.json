{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'MensalidadesAtrasadas',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      alunos: [],\n      atrasado: [],\n      atrasados: [],\n      nome: '',\n      mesAtual: '',\n      diaAtual: '',\n      anoAtual: '',\n      dataAtual: '',\n      mesAno: ''\n    };\n  },\n  methods: {\n    //FUNÇÃO PARA MANDAR TODOS ID ALUNOS PARA VERIFICAR PAGAMENTO NA TABELA RECEBIMENTOS \n\n    //RETORNAR TODOS ALUNOS TIPO PLANO MENSALIDADE\n    getAllAlunosMensalidade() {\n      axios.get(`${this.URL}/alunosmensalidade`).then(response => {\n        this.alunos = response.data;\n      });\n      //this.limpaDados()\n      console.log(this.alunos);\n    },\n    getAtrasados() {\n      axios.get(`${this.URL}/recebimentoatrasadas/` + this.mesAtual).then(response => {\n        this.atrasados = response.data;\n        console.log(response.data);\n      });\n    },\n    //FUNÇÃO TESTE\n    teste() {\n      console.log(this.alunos);\n      for (let i = 0; i < this.alunos.length; i++) {\n        axios.get(`${this.URL}/recebimentoatrasadas/` + this.alunos[i].id_aluno + '/' + this.mesAno).then(response => {\n          this.atrasado = response.data[0];\n          console.log(this.atrasado);\n        });\n        this.pago[i] = this.atrasado[0];\n        //this.limpaDados();\n      }\n    },\n\n    pegarDataAtual() {\n      const data = new Date();\n      const dia = String(data.getDate()).padStart(2, '0');\n      const mes = String(data.getMonth() + 1).padStart(2, '0');\n      const ano = String(data.getFullYear());\n      const atual = `${dia}/${mes}/${ano}`;\n      this.mesAtual = mes;\n      this.diaAtual = dia;\n      this.anoAtual = ano;\n      this.dataAtual = atual;\n      this.mesAno = mes + '-' + ano;\n    },\n    limpaDados() {\n      this.atrasado = '';\n    }\n  },\n  mounted() {\n    this.getAllAlunosMensalidade();\n    this.pegarDataAtual();\n    //this.getAtrasados();\n    this.teste();\n  }\n};","map":{"version":3,"names":["axios","name","data","URL","alunos","atrasado","atrasados","nome","mesAtual","diaAtual","anoAtual","dataAtual","mesAno","methods","getAllAlunosMensalidade","get","then","response","console","log","getAtrasados","teste","i","length","id_aluno","pago","pegarDataAtual","Date","dia","String","getDate","padStart","mes","getMonth","ano","getFullYear","atual","limpaDados","mounted"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\financeiro\\mensalidadesAtrasadas.vue"],"sourcesContent":["<template>\r\n    <h3>Mensalidades em atraso</h3>\r\n    <form class=\"form-inline\">\r\n        <h5>Busque pelo o nome do aluno que deseja verificar!</h5>\r\n        <div class=\"form-group mb-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Digite o nome do aluno</label >\r\n            <input type=\"text\" class=\"form-control\" v-model=\"nome\">\r\n        </div>\r\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"buscar()\">\r\n            <i class=\"fa fa-search\"></i> BUSCAR</button>\r\n    </form>\r\n    <br>\r\n    <div class=\"alunos-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Nome</th>\r\n                    <th scope=\"col\">CPF</th>\r\n                    <th scope=\"col\">Plano</th>\r\n                    <th scope=\"col\">Valor Plano</th>\r\n                    <th scope=\"col\">Dia Vencimento</th>\r\n                    <th id=\"mesAtraso\" scope=\"col\">Mês em Atraso</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"receb in atrasados\" :key=\"receb.id_mensalidade\">\r\n                    <th scope=\"row\">{{ receb.nome_aluno }}</th>\r\n                    <td>{{ receb.cpf_aluno }}</td>\r\n                    <td>{{ receb.nome_plano }}</td>\r\n                    <td>R$ {{ receb.valor_plano.toFixed(2) }}</td>\r\n                    <td>{{ receb.diapag_format }}</td>\r\n                    <td>{{ this.mesAtual }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'MensalidadesAtrasadas',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            alunos: [],\r\n            atrasado: [],\r\n            atrasados: [],\r\n            nome: '',\r\n            mesAtual: '',\r\n            diaAtual: '',\r\n            anoAtual: '',\r\n            dataAtual: '',\r\n            mesAno: '',\r\n        }\r\n    },\r\n    methods: {\r\n        //FUNÇÃO PARA MANDAR TODOS ID ALUNOS PARA VERIFICAR PAGAMENTO NA TABELA RECEBIMENTOS \r\n        \r\n        //RETORNAR TODOS ALUNOS TIPO PLANO MENSALIDADE\r\n        getAllAlunosMensalidade() {\r\n            axios.get(`${this.URL}/alunosmensalidade`).then(response => {\r\n                this.alunos = response.data\r\n            })\r\n            //this.limpaDados()\r\n            console.log(this.alunos);\r\n        },\r\n        getAtrasados() {\r\n            axios.get(`${this.URL}/recebimentoatrasadas/`+this.mesAtual).then(response => {\r\n                this.atrasados = response.data\r\n                console.log(response.data);\r\n            })\r\n        },\r\n        //FUNÇÃO TESTE\r\n        teste() {\r\n            console.log(this.alunos);\r\n            for(let i = 0; i < this.alunos.length; i++){\r\n                axios.get(`${this.URL}/recebimentoatrasadas/`+this.alunos[i].id_aluno+'/'+this.mesAno).then(response => {\r\n                this.atrasado = response.data[0]\r\n                console.log(this.atrasado);\r\n                })\r\n                this.pago[i] = this.atrasado[0];\r\n                //this.limpaDados();\r\n            }\r\n            \r\n        },\r\n        pegarDataAtual() {\r\n            const data = new Date()\r\n            const dia = String(data.getDate()).padStart(2, '0')\r\n            const mes = String(data.getMonth() + 1).padStart(2, '0')\r\n            const ano = String(data.getFullYear())\r\n            const atual = `${dia}/${mes}/${ano}`\r\n            this.mesAtual = mes\r\n            this.diaAtual = dia\r\n            this.anoAtual = ano\r\n            this.dataAtual = atual\r\n            this.mesAno = mes+'-'+ano\r\n\r\n        },\r\n        limpaDados() {\r\n            this.atrasado = '';\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getAllAlunosMensalidade();\r\n        this.pegarDataAtual();\r\n        //this.getAtrasados();\r\n        this.teste();\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    th#mesAtraso{\r\n        background-color: rgb(192, 56, 56);\r\n    }\r\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,uBAAuB;EAC7BC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL;;IAEA;IACAC,uBAAuBA,CAAA,EAAG;MACtBd,KAAK,CAACe,GAAG,CAAE,GAAE,IAAI,CAACZ,GAAI,oBAAmB,CAAC,CAACa,IAAI,CAACC,QAAO,IAAK;QACxD,IAAI,CAACb,MAAK,GAAIa,QAAQ,CAACf,IAAG;MAC9B,CAAC;MACD;MACAgB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,MAAM,CAAC;IAC5B,CAAC;IACDgB,YAAYA,CAAA,EAAG;MACXpB,KAAK,CAACe,GAAG,CAAE,GAAE,IAAI,CAACZ,GAAI,wBAAuB,GAAC,IAAI,CAACK,QAAQ,CAAC,CAACQ,IAAI,CAACC,QAAO,IAAK;QAC1E,IAAI,CAACX,SAAQ,GAAIW,QAAQ,CAACf,IAAG;QAC7BgB,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACf,IAAI,CAAC;MAC9B,CAAC;IACL,CAAC;IACD;IACAmB,KAAKA,CAAA,EAAG;MACJH,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,MAAM,CAAC;MACxB,KAAI,IAAIkB,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAAClB,MAAM,CAACmB,MAAM,EAAED,CAAC,EAAE,EAAC;QACvCtB,KAAK,CAACe,GAAG,CAAE,GAAE,IAAI,CAACZ,GAAI,wBAAuB,GAAC,IAAI,CAACC,MAAM,CAACkB,CAAC,CAAC,CAACE,QAAQ,GAAC,GAAG,GAAC,IAAI,CAACZ,MAAM,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;UAC