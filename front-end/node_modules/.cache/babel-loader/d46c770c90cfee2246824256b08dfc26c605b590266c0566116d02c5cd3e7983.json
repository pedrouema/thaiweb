{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"VerMensalidadesRecebidas\",\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      recebidas: [],\n      dataIni: '',\n      dataFim: ''\n    };\n  },\n  methods: {\n    getAllRecebidas() {\n      axios.get(`${this.URL}/getallrecebidas`).then(response => {\n        this.recebidas = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    buscarRecebidas() {\n      if (this.dataIni == '' && this.dataFim == '') {\n        this.getAllRecebidas();\n      } else {\n        this.retornaRecebidasEntreDatas();\n        console.log('Passou por data despesas quitadas');\n        //this.limpaDados()\n      }\n    },\n\n    retornaRecebidasEntreDatas() {\n      axios.get(`${this.URL}/recebidasentredatas/${this.dataIni}/${this.dataFim}`).then(response => {\n        this.recebidas = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    showAlertApagar(id) {\n      // Use sweetalert2\n      const swalWithBootstrapButtons = this.$swal.mixin({\n        customClass: {\n          confirmButton: 'btn btn-success',\n          cancelButton: 'btn btn-danger'\n        },\n        buttonsStyling: false\n      });\n      swalWithBootstrapButtons.fire({\n        title: 'Deseja realmente apagar esta fatura recebida?',\n        text: \"\",\n        icon: 'question',\n        showCancelButton: true,\n        confirmButtonText: 'Sim, apagar agora!',\n        cancelButtonText: 'Não, cancelar!',\n        reverseButtons: true\n      }).then(result => {\n        if (result.isConfirmed) {\n          swalWithBootstrapButtons.fire('FATURA RECEBIDA APAGADA COM SUCESSO!', '', 'success');\n          this.apagarDespesa(id);\n        } else if ( /* Read more about handling dismissals below */\n        result.dismiss === this.$swal.DismissReason.cancel) {\n          swalWithBootstrapButtons.fire('Cancelado! ', 'fatura recebida não foi apagada! ', 'error');\n        }\n      });\n    },\n    apagarDespesa(id) {\n      axios.delete(this.URL + \"/recebimento/delete/\" + id).then(() => {\n        this.buscarRecebidas();\n      });\n    },\n    limpaDados() {\n      this.dataIni = '';\n      this.dataFim = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","URL","recebidas","dataIni","dataFim","methods","getAllRecebidas","get","then","response","catch","error","console","log","buscarRecebidas","retornaRecebidasEntreDatas","showAlertApagar","id","swalWithBootstrapButtons","$swal","mixin","customClass","confirmButton","cancelButton","buttonsStyling","fire","title","text","icon","showCancelButton","confirmButtonText","cancelButtonText","reverseButtons","result","isConfirmed","apagarDespesa","dismiss","DismissReason","cancel","delete","limpaDados"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\financeiro\\verMensalidadesRecebidas.vue"],"sourcesContent":["<template>\r\n    <h3>Gerenciamento de Recebimentos</h3>\r\n    <form class=\"row g-3\">\r\n        <h5>Selecione as datas inicio e fim para buscar as mensalidades recebidas.</h5>\r\n        <div class=\"col-md-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Data Inicio</label >\r\n            <input type=\"date\" class=\"form-control\" v-model=\"dataIni\">\r\n        </div>\r\n        <div class=\"col-md-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Data Fim</label >\r\n            <input type=\"date\" class=\"form-control\" v-model=\"dataFim\">\r\n        </div>\r\n        <div class=\"col-12\">\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"buscarRecebidas()\">\r\n                <i class=\"fa fa-search\"></i> BUSCAR</button>\r\n        </div>\r\n    </form>\r\n    <br>\r\n    <div class=\"recebidas-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Nome</th>\r\n                    <th scope=\"col\">CPF</th>\r\n                    <th scope=\"col\">Data do Recebimento</th>\r\n                    <th scope=\"col\">Valor Recebido</th>\r\n                    <th scope=\"col\">Ação</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"recebida in recebidas\" :key=\"recebida.id_mensalidade\">\r\n                    <th scope=\"row\">{{ recebida.nome_aluno }}</th>\r\n                    <td>{{ recebida.cpf_aluno }}</td>\r\n                    <td>{{ recebida.data_recebimento }}</td>\r\n                    <td>R${{ recebida.valor_recebimento.toFixed(2) }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"showAlertApagar(recebida.id_mensalidade)\">\r\n                            <i class=\"fa fa-trash\"> </i>\r\n                             Apagar (Não Foi Recebido)\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: \"VerMensalidadesRecebidas\",\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            recebidas: [],\r\n            dataIni: '',\r\n            dataFim: '',\r\n        }\r\n    },\r\n    methods: {\r\n        getAllRecebidas() {\r\n            axios.get(`${this.URL}/getallrecebidas`).then(response => {\r\n                this.recebidas = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        buscarRecebidas() {\r\n            if(this.dataIni == '' && this.dataFim == ''){\r\n                this.getAllRecebidas()\r\n            }\r\n            else{\r\n                this.retornaRecebidasEntreDatas()\r\n                console.log('Passou por data despesas quitadas');\r\n                //this.limpaDados()\r\n            }\r\n        },\r\n        retornaRecebidasEntreDatas(){\r\n            axios.get(`${this.URL}/recebidasentredatas/${this.dataIni}/${this.dataFim}`).then(response => {\r\n                this.recebidas = response.data\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        showAlertApagar(id) {\r\n            // Use sweetalert2\r\n            const swalWithBootstrapButtons = this.$swal.mixin({\r\n            customClass: {\r\n                confirmButton: 'btn btn-success',\r\n                cancelButton: 'btn btn-danger'\r\n            },\r\n            buttonsStyling: false\r\n            })\r\n\r\n            swalWithBootstrapButtons.fire({\r\n            title: 'Deseja realmente apagar esta fatura recebida?',\r\n            text: \"\",\r\n            icon: 'question',\r\n            showCancelButton: true,\r\n            confirmButtonText: 'Sim, apagar agora!',\r\n            cancelButtonText: 'Não, cancelar!',\r\n            reverseButtons: true\r\n            }).then((result) => {\r\n            if (result.isConfirmed) {\r\n                swalWithBootstrapButtons.fire(\r\n                'FATURA RECEBIDA APAGADA COM SUCESSO!',\r\n                '',\r\n                'success'\r\n                )\r\n                this.apagarDespesa(id)\r\n            } else if (\r\n                /* Read more about handling dismissals below */\r\n                result.dismiss === this.$swal.DismissReason.cancel\r\n            ) {\r\n                swalWithBootstrapButtons.fire(\r\n                'Cancelado! ',\r\n                'fatura recebida não foi apagada! ',\r\n                'error'\r\n                )\r\n            }\r\n            })\r\n        },\r\n        apagarDespesa(id) {\r\n            axios.delete(this.URL+\"/recebimento/delete/\"+id).then(()=>{\r\n                this.buscarRecebidas()\r\n            })\r\n        },\r\n        limpaDados() {\r\n            this.dataIni = '';\r\n            this.dataFim = '';\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAgDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,0BAA0B;EAChCC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,eAAeA,CAAA,EAAG;MACdR,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,kBAAiB,CAAC,CAACO,IAAI,CAACC,QAAO,IAAK;QACtD,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACT,IAAG;MACjC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDG,eAAeA,CAAA,EAAG;MACd,IAAG,IAAI,CAACX,OAAM,IAAK,EAAC,IAAK,IAAI,CAACC,OAAM,IAAK,EAAE,EAAC;QACxC,IAAI,CAACE,eAAe,EAAC;MACzB,OACI;QACA,IAAI,CAACS,0BAA0B,EAAC;QAChCH,OAAO,CAACC,GAAG,CAAC,mCAAmC,CAAC;QAChD;MACJ;IACJ,CAAC;;IACDE,0BAA0BA,CAAA,EAAE;MACxBjB,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,wBAAuB,IAAI,CAACE,OAAQ,IAAG,IAAI,CAACC,OAAQ,EAAC,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QAC1F,IAAI,CAACP,SAAQ,GAAIO,QAAQ,CAACT,IAAG;MACjC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDK,eAAeA,CAACC,EAAE,EAAE;MAChB;MACA,MAAMC,wBAAuB,GAAI,IAAI,CAACC,KAAK,CAACC,KAAK,CAAC;QAClDC,WAAW,EAAE;UACTC,aAAa,EAAE,iBAAiB;UAChCC,YAAY,EAAE;QAClB,CAAC;QACDC,cAAc,EAAE;MAChB,CAAC;MAEDN,wBAAwB,CAACO,IAAI,CAAC;QAC9BC,KAAK,EAAE,+CAA+C;QACtDC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,UAAU;QAChBC,gBAAgB,EAAE,IAAI;QACtBC,iBAAiB,EAAE,oBAAoB;QACvCC,gBAAgB,EAAE,gBAAgB;QAClCC,cAAc,EAAE;MAChB,CAAC,CAAC,CAACxB,IAAI,CAAEyB,MAAM,IAAK;QACpB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACpBhB,wBAAwB,CAACO,IAAI,CAC7B,sCAAsC,EACtC,EAAE,EACF,SAAQ,CACR;UACA,IAAI,CAACU,aAAa,CAAClB,EAAE;QACzB,OAAO,KACH;QACAgB,MAAM,CAACG,OAAM,KAAM,IAAI,CAACjB,KAAK,CAACkB,aAAa,CAACC,MAAK,EACnD;UACEpB,wBAAwB,CAACO,IAAI,CAC7B,aAAa,EACb,mCAAmC,EACnC,OAAM,CACN;QACJ;MACA,CAAC;IACL,CAAC;IACDU,aAAaA,CAAClB,EAAE,EAAE;MACdnB,KAAK,CAACyC,MAAM,CAAC,IAAI,CAACtC,GAAG,GAAC,sBAAsB,GAA