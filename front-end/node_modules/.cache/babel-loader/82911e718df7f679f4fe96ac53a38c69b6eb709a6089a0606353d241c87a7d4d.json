{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'GerenciarDespesas',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      despesas: [],\n      nome: '',\n      valor: '',\n      dataVenc: '',\n      dataIni: '',\n      dataFim: '',\n      despesaId: 0,\n      tabela: true,\n      QualTabela: ''\n    };\n  },\n  methods: {\n    showAlertQuitar() {\n      // Use sweetalert2\n      $swal({\n        title: \"Deseja realmente quitar esta despesa?\",\n        text: \"\",\n        icon: \"warning\",\n        buttons: true,\n        dangerMode: true\n      }).then(willDelete => {\n        if (willDelete) {\n          swal(\"Despesa quitada com sucesso!\", {\n            icon: \"success\"\n          });\n        } else {\n          swal(\"Quitação cancelada!\");\n        }\n      });\n    },\n    showAlertVoltar() {\n      // Use sweetalert2\n      this.$swal('Deseja realmete voltar está despesa para as (Não quitadas)?');\n    },\n    buscarDespesas() {\n      if (this.QualTabela == 'Ver despesas Quitadas') {\n        this.tabela = false;\n        this.getAllDespesasQuitadas();\n      } else {\n        this.tabela = true;\n        this.getAllDespesas();\n      }\n    },\n    getAllDespesas() {\n      axios.get(`${this.URL}/despesas`).then(response => {\n        this.despesas = response.data;\n        console.log(this.despesas);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    getAllDespesasQuitadas() {\n      axios.get(`${this.URL}/despesasquitadas`).then(response => {\n        this.despesas = response.data;\n        console.log(this.despesas);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    voltarDespesa(id) {\n      const despesa = {\n        quitada_despesa: false\n      };\n      axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\n        console.log(response);\n        this.showAlertVoltar();\n        this.getAllDespesasQuitadas();\n      });\n      this.limpaDadosFormulario();\n    },\n    quitarDespesa(id) {\n      this.editar = true;\n      const despesa = {\n        quitada_despesa: true\n      };\n      axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\n        console.log(response);\n        this.showAlertQuitar();\n        this.getAllDespesas();\n      });\n      this.limpaDadosFormulario();\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"names":["axios","name","data","URL","despesas","nome","valor","dataVenc","dataIni","dataFim","despesaId","tabela","QualTabela","methods","showAlertQuitar","$swal","title","text","icon","buttons","dangerMode","then","willDelete","swal","showAlertVoltar","buscarDespesas","getAllDespesasQuitadas","getAllDespesas","get","response","console","log","catch","error","voltarDespesa","id","despesa","quitada_despesa","put","limpaDadosFormulario","quitarDespesa","editar","mounted"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\financeiro\\gerenciarDespesas.vue"],"sourcesContent":["<template>\r\n    <h3>Gerenciamento de Despesas</h3>\r\n    <br/>\r\n    <form class=\"row g-3\">\r\n        <h5>Selecione o filtro de ação e as datas inicio e fim para buscar as despesas.</h5>\r\n        <div class=\"col-md-4\" >\r\n                <label for=\"inputState\" class=\"form-label\">Selecione a ação desejada</label>\r\n                <select class=\"form-select\" v-model=\"QualTabela\">\r\n                    <option value=\"Quitar Despesa\">Quitar Despesa</option>\r\n                    <option value=\"Ver despesas Quitadas\">Ver Despesas Quitadas</option>\r\n                </select>\r\n            </div>\r\n        <div class=\"col-md-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Data Inicio</label >\r\n            <input type=\"date\" class=\"form-control\" v-model=\"dataIni\">\r\n        </div>\r\n        <div class=\"col-md-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Data Fim</label >\r\n            <input type=\"date\" class=\"form-control\" v-model=\"dataFim\">\r\n        </div>\r\n        <div class=\"col-12\">\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"buscarDespesas()\">\r\n                <i class=\"fa fa-search\"></i> BUSCAR</button>\r\n        </div>\r\n    </form>\r\n    <br/>\r\n    <div class=\"alunos-table\">\r\n        <!-- Tabela de Despesas à Quitar -->\r\n        <table class=\"table\" v-show=\"tabela\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Despesa</th>\r\n                    <th scope=\"col\">Valor</th>\r\n                    <th scope=\"col\">Data Vencimento</th>\r\n                    <th scope=\"col\">Ação</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"despesa in despesas\" :key=\"despesa.id_despesa\">\r\n                    <th scope=\"row\">{{ despesa.nome_despesa }}</th>\r\n                    <td>R$ {{ despesa.valor_despesa.toFixed(2) }}</td>\r\n                    <td>{{ despesa.vencimento_despesa }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-success\" @click=\"showAlertQuitar()\">\r\n                            <!-- @click=\"quitarDespesa(despesa.id_despesa)\" -->\r\n                            <i class=\"fa fa-dollar\"> </i>\r\n                             Quitar\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n        <!-- Tabela de Despesas já Pagas -->\r\n        <table class=\"table\" v-show=\"!tabela\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Despesa</th>\r\n                    <th scope=\"col\">Valor</th>\r\n                    <th scope=\"col\">Data Vencimento</th>\r\n                    <th scope=\"col\">Data Pagamento</th>\r\n                    <th scope=\"col\">Valor Pago</th>\r\n                    <th scope=\"col\">Ação</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"despesa in despesas\" :key=\"despesa.id_despesa\">\r\n                    <th scope=\"row\">{{ despesa.nome_despesa }}</th>\r\n                    <td>R$ {{ despesa.valor_despesa.toFixed(2) }}</td>\r\n                    <td>{{ despesa.vencimento_despesa }}</td>\r\n                    <td>{{ despesa.datapag_despesa }}</td>\r\n                    <td>{{ despesa.valorpag_despesa }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"voltarDespesa(despesa.id_despesa)\">\r\n                            <i class=\"fa fa-reply-all\"> </i>\r\n                             Voltar (Não Foi Paga)\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'GerenciarDespesas',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            despesas: [],\r\n            nome: '',\r\n            valor: '',\r\n            dataVenc: '',\r\n            dataIni: '',\r\n            dataFim: '',\r\n            despesaId: 0,\r\n            tabela: true,\r\n            QualTabela: '',\r\n        }\r\n    },\r\n    methods: {\r\n        showAlertQuitar() {\r\n            // Use sweetalert2\r\n            $swal({\r\n                title: \"Deseja realmente quitar esta despesa?\",\r\n                text: \"\",\r\n                icon: \"warning\",\r\n                buttons: true,\r\n                dangerMode: true,\r\n            })\r\n            .then((willDelete) => {\r\n            if (willDelete) {\r\n                swal(\"Despesa quitada com sucesso!\", {\r\n                icon: \"success\",\r\n                });\r\n            } else {\r\n                swal(\"Quitação cancelada!\");\r\n            }\r\n            });\r\n        },\r\n        showAlertVoltar() {\r\n            // Use sweetalert2\r\n            this.$swal('Deseja realmete voltar está despesa para as (Não quitadas)?');\r\n        },\r\n        buscarDespesas(){\r\n            if(this.QualTabela == 'Ver despesas Quitadas'){\r\n                this.tabela = false\r\n                this.getAllDespesasQuitadas()\r\n            }\r\n            else{\r\n                this.tabela = true\r\n                this.getAllDespesas()\r\n            }\r\n        },\r\n        getAllDespesas() {\r\n            axios.get(`${this.URL}/despesas`).then(response => {\r\n                this.despesas = response.data\r\n                console.log(this.despesas);\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        getAllDespesasQuitadas() {\r\n            axios.get(`${this.URL}/despesasquitadas`).then(response => {\r\n                this.despesas = response.data\r\n                console.log(this.despesas);\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        voltarDespesa(id){\r\n            const despesa = {\r\n                quitada_despesa: false,\r\n            }\r\n            axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\r\n                console.log(response);\r\n                this.showAlertVoltar();\r\n                this.getAllDespesasQuitadas();\r\n            });\r\n            this.limpaDadosFormulario();\r\n        },\r\n        quitarDespesa(id){\r\n            this.editar = true\r\n            const despesa = {\r\n                quitada_despesa: true,\r\n            }\r\n            axios.put(`${this.URL}/despesas/quitar/${id}`, despesa).then(response => {\r\n                console.log(response);\r\n                this.showAlertQuitar();\r\n                this.getAllDespesas();\r\n            });\r\n            this.limpaDadosFormulario();\r\n        },\r\n    },\r\n    mounted() {\r\n        \r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAoFA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,CAAC;MACZC,MAAM,EAAE,IAAI;MACZC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,eAAeA,CAAA,EAAG;MACd;MACAC,KAAK,CAAC;QACFC,KAAK,EAAE,uCAAuC;QAC9CC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,SAAS;QACfC,OAAO,EAAE,IAAI;QACbC,UAAU,EAAE;MAChB,CAAC,EACAC,IAAI,CAAEC,UAAU,IAAK;QACtB,IAAIA,UAAU,EAAE;UACZC,IAAI,CAAC,8BAA8B,EAAE;YACrCL,IAAI,EAAE;UACN,CAAC,CAAC;QACN,OAAO;UACHK,IAAI,CAAC,qBAAqB,CAAC;QAC/B;MACA,CAAC,CAAC;IACN,CAAC;IACDC,eAAeA,CAAA,EAAG;MACd;MACA,IAAI,CAACT,KAAK,CAAC,6DAA6D,CAAC;IAC7E,CAAC;IACDU,cAAcA,CAAA,EAAE;MACZ,IAAG,IAAI,CAACb,UAAS,IAAK,uBAAuB,EAAC;QAC1C,IAAI,CAACD,MAAK,GAAI,KAAI;QAClB,IAAI,CAACe,sBAAsB,EAAC;MAChC,OACI;QACA,IAAI,CAACf,MAAK,GAAI,IAAG;QACjB,IAAI,CAACgB,cAAc,EAAC;MACxB;IACJ,CAAC;IACDA,cAAcA,CAAA,EAAG;MACb3B,KAAK,CAAC4B,GAAG,CAAE,GAAE,IAAI,CAACzB,GAAI,WAAU,CAAC,CAACkB,IAAI,CAACQ,QAAO,IAAK;QAC/C,IAAI,CAACzB,QAAO,GAAIyB,QAAQ,CAAC3B,IAAG;QAC5B4B,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC3B,QAAQ,CAAC;MAC9B,CAAC,EACA4B,KAAK,CAACC,KAAI,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDP,sBAAsBA,CAAA,EAAG;MACrB1B,KAAK,CAAC4B,GAAG,CAAE,GAAE,IAAI,CAACzB,GAAI,mBAAkB,CAAC,CAACkB,IAAI,CAACQ,QAAO,IAAK;QACvD,IAAI,CAACzB,QAAO,GAAIyB,QAAQ,CAAC3B,IAAG;QAC5B4B,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC3B,QAAQ,CAAC;MAC9B,CAAC,EACA4B,KAAK,CAACC,KAAI,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDC,aAAaA,CAACC,EAAE,EAAC;MACb,MAAMC,OAAM,GAAI;QACZC,eAAe,EAAE;MACrB;MACArC,KAAK,CAACsC,GAAG,CAAE,GAAE,IAAI,CAACnC,GAAI,oBAAmBgC