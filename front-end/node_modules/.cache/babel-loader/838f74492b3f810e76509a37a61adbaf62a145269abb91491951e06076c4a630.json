{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'TurmaAdmin',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      instrutores: [],\n      turmas: [],\n      dias: '',\n      turmaId: 0,\n      nome: '',\n      instrutorId: 0,\n      instrutor: 0,\n      editar: false,\n      hrtreino: '',\n      qtdeSemana: '',\n      seg: false,\n      ter: false,\n      qua: false,\n      qui: false,\n      sex: false,\n      sab: false\n    };\n  },\n  methods: {\n    getAllTurmas() {\n      axios.get(`${this.URL}/turmas`).then(response => {\n        this.turmas = response.data;\n        console.log(this.turmas);\n        for (i = 0; i < this.turmas.length; i++) {\n          if (turmas[i].segunda) this.dias = 'segunda';\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    createTurma() {\n      const turma = {\n        nome_turma: this.nome,\n        id_instrutor: this.instrutor,\n        qtdesemanal_turma: this.qtdeSemana,\n        horario_turma: this.hrtreino,\n        segunda: this.seg,\n        terca: this.ter,\n        quarta: this.qua,\n        quinta: this.qui,\n        sexta: this.sex,\n        sabado: this.sab\n      };\n      console.log(turma);\n      axios.post(`${this.URL}/turmas`, turma).then(response => {\n        console.log(response);\n        this.getAllTurmas();\n      });\n      console.log('seg:' + this.seg);\n      console.log('ter:' + this.ter);\n      console.log('qua:' + this.qua);\n      console.log('qui:' + this.qui);\n      console.log('sex:' + this.sex);\n      console.log('sab:' + this.sab);\n      this.limpaDadosFormulario();\n    },\n    carregarDadosTurma(id) {\n      this.editar = true;\n      axios.get(`${this.URL}/turmas/getone/` + id).then(response => {\n        const turma = response.data[0];\n        this.turmaId = turma.id_turma;\n        this.nome = turma.nome_turma;\n        this.instrutor = turma.id_instrutor;\n        console.log(response.data[0]);\n      });\n    },\n    salvarDados() {\n      const turma = {\n        nome_turma: this.nome,\n        id_instrutor: this.instrutor\n      };\n      console.log(turma);\n      axios.put(`${this.URL}/turmas/${this.turmaId}`, turma).then(response => {\n        console.log(response);\n        this.getAllTurmas();\n      });\n      this.limpaDadosFormulario();\n    },\n    deleteTurma(id) {\n      axios.delete(this.URL + \"/turmas/delete/\" + id).then(() => {\n        this.getAllTurmas();\n      });\n    },\n    getAllInstrutoresOptions() {\n      axios.get(`${this.URL}/instrutores/options`).then(response => {\n        this.instrutores = response.data;\n        console.log(this.instrutores);\n      });\n    },\n    limpaDadosFormulario() {\n      this.turmaId = 0;\n      this.nome = '';\n      this.instrutorId = 0;\n      this.instrutor = 0;\n      this.editar = false;\n      this.hrtreino = '';\n      this.qtdeSemana = '';\n      this.seg = false;\n      this.ter = false;\n      this.qua = false;\n      this.qui = false;\n      this.sex = false;\n      this, sab = false;\n    }\n  },\n  mounted() {\n    this.getAllTurmas();\n    this.getAllInstrutoresOptions();\n  }\n};","map":{"version":3,"names":["axios","name","data","URL","instrutores","turmas","dias","turmaId","nome","instrutorId","instrutor","editar","hrtreino","qtdeSemana","seg","ter","qua","qui","sex","sab","methods","getAllTurmas","get","then","response","console","log","i","length","segunda","catch","error","createTurma","turma","nome_turma","id_instrutor","qtdesemanal_turma","horario_turma","terca","quarta","quinta","sexta","sabado","post","limpaDadosFormulario","carregarDadosTurma","id","id_turma","salvarDados","put","deleteTurma","delete","getAllInstrutoresOptions","mounted"],"sources":["C:\\Users\\Barbára Tagliari\\Desktop\\thaiweb\\front-end\\src\\components\\admin\\TurmaAdmin.vue"],"sourcesContent":["<template>\r\n    <div class=\"turma-admin\">\r\n        <h3>Gerenciamento de Turma</h3>\r\n        <form class=\"row g-3\">\r\n            <div class=\"col-md-6\">\r\n                <label for=\"inputEmail4\" class=\"form-label\">Nome da Turma</label>\r\n                <input type=\"text\" class=\"form-control\" placeholder=\"exemplo: Turma 18:00\" v-model=\"nome\">\r\n            </div>\r\n            <div class=\"col-md-4\">\r\n                <label for=\"inputState\" class=\"form-label\">Instrutor</label>\r\n                <select class=\"form-select\" v-model=\"instrutor\">\r\n                    <option value=\"\">Selecione o Instrutor</option>\r\n                    <option v-for=\"instrutor in instrutores\" :key=\"instrutor.id_instrutor\" v-bind:value=\"instrutor.id_instrutor\">\r\n                        {{ instrutor.nome_instrutor }}\r\n                    </option>\r\n                </select>\r\n            </div>\r\n            <div class=\"col-md-3\">\r\n                <label for=\"inputZip\" class=\"form-label\">Selecione quantas vezes por semana</label>\r\n                <select class=\"form-select\" v-model=\"qtdeSemana\">\r\n                    <option value=\"1\">1</option>\r\n                    <option value=\"2\">2</option>\r\n                    <option value=\"3\">3</option>\r\n                    <option value=\"4\">4</option>\r\n                    <option value=\"5\">5</option>\r\n                </select>\r\n            </div>\r\n            <div class=\"col-md-3\">\r\n                <label for=\"inputZip\" class=\"form-label\">Selecione o horario do inicio do treino</label>\r\n                <select class=\"form-select\" v-model=\"hrtreino\">\r\n                    <option value=\"08:00\">08:00</option>\r\n                    <option value=\"08:30\">08:30</option>\r\n                    <option value=\"09:00\">09:00</option>\r\n                    <option value=\"09:30\">09:30</option>\r\n                    <option value=\"10:00\">10:00</option>\r\n                    <option value=\"10:30\">10:30</option>\r\n                    <option value=\"11:00\">11:00</option>\r\n                    <option value=\"14:00\">14:00</option>\r\n                    <option value=\"14:30\">14:30</option>\r\n                    <option value=\"15:00\">15:00</option>\r\n                    <option value=\"15:30\">15:30</option>\r\n                    <option value=\"16:00\">16:00</option>\r\n                    <option value=\"16:30\">16:30</option>\r\n                    <option value=\"17:00\">17:00</option>\r\n                    <option value=\"17:30\">17:30</option>\r\n                    <option value=\"18:00\">18:00</option>\r\n                    <option value=\"18:30\">18:30</option>\r\n                    <option value=\"19:00\">19:00</option>\r\n                    <option value=\"19:30\">19:30</option>\r\n                    <option value=\"20:00\">20:00</option>\r\n                    <option value=\"20:30\">20:30</option>\r\n                    <option value=\"21:00\">21:00</option>\r\n                </select>\r\n            </div>\r\n        </form>\r\n        <br/>\r\n        <label for=\"inputZip\" class=\"form-label\">Selecione os dias de treino da turma: </label><br>\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox1\" value=\"option1\" v-model=\"seg\"/>\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox1\">Segunda</label>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox2\" value=\"option2\" v-model=\"ter\"/>\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox2\">Terça</label>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox3\" value=\"option3\" v-model=\"qua\"/>\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox3\">Quarta</label>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox4\" value=\"option4\" v-model=\"qui\"/>\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox4\">Quinta</label>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox5\" value=\"option5\" v-model=\"sex\"/>\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox5\">Sexta</label>\r\n        </div>\r\n\r\n        <div class=\"form-check form-check-inline\">\r\n            <input class=\"form-check-input\" type=\"checkbox\" id=\"inlineCheckbox6\" value=\"option6\" v-model=\"sab\" />\r\n            <label class=\"form-check-label\" for=\"inlineCheckbox6\">Sábado</label>\r\n        </div>\r\n        <br/>\r\n        <br/>\r\n        \r\n        <div class=\"col-12\">\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"createTurma()\" v-show=\"!editar\" >+ SALVAR</button>\r\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"salvarDados()\" v-show=\"editar\">+ EDITAR</button>\r\n            <button  type=\"button\" class=\"btn btn-danger\" @click=\"limpaDadosFormulario()\" >CANCELAR</button>\r\n        </div>\r\n    </div>\r\n    <hr/>\r\n    <div class=\"turma-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">ID</th>\r\n                    <th scope=\"col\">Nome da Turma</th>\r\n                    <th scope=\"col\">Horario</th>\r\n                    <!-- <th scope=\"col\">Dia(s)</th> -->\r\n                    <th scope=\"col\">Instrutor</th>\r\n                    <th scope=\"col\">Ações</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"turma in turmas\" :key=\"turma.id_turma\">\r\n                    <th scope=\"row\">{{ turma.id_turma }}</th>\r\n                    <td>{{ turma.nome_turma+' '+turma.qtdesemanal_turma+'x' }}</td>\r\n                    <td>{{ turma.horario_turma }}</td>\r\n                    <td>{{ this.dias }}</td>\r\n                    <td>{{ turma.nome_instrutor }}</td>\r\n                    <td>\r\n                        <button type=\"button\" class=\"btn btn-success\" @click=\"carregarDadosTurma(turma.id_turma)\">\r\n                            <i class=\"fa fa-pencil\"> </i>\r\n                             Editar\r\n                        </button>\r\n                        <button type=\"button\" class=\"btn btn-danger\" @click=\"deleteTurma(turma.id_turma)\">\r\n                            <i class=\"fa fa-trash\"> </i>\r\n                             Excluir\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\n\r\nexport default {\r\n    name: 'TurmaAdmin',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            instrutores:[],\r\n            turmas: [],\r\n            dias: '',\r\n            turmaId: 0,\r\n            nome: '',\r\n            instrutorId: 0,\r\n            instrutor: 0,\r\n            editar: false,\r\n\r\n            hrtreino: '',\r\n            qtdeSemana: '',\r\n            seg: false,\r\n            ter: false,\r\n            qua: false,\r\n            qui: false,\r\n            sex: false,\r\n            sab: false,\r\n        }\r\n    },\r\n    methods: {\r\n        getAllTurmas() {\r\n            axios.get(`${this.URL}/turmas`).then(response => {\r\n                this.turmas = response.data\r\n                console.log(this.turmas);\r\n                for(i = 0; i < this.turmas.length; i++)\r\n                {\r\n                    if(turmas[i].segunda)\r\n                        this.dias = 'segunda'\r\n                }\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n            })\r\n        },\r\n        createTurma() {\r\n            const turma = {\r\n                nome_turma: this.nome,\r\n                id_instrutor: this.instrutor,\r\n                qtdesemanal_turma: this.qtdeSemana,\r\n                horario_turma: this.hrtreino,\r\n                segunda: this.seg,\r\n                terca: this.ter,\r\n                quarta: this.qua,\r\n                quinta: this.qui,\r\n                sexta: this.sex,\r\n                sabado: this.sab\r\n            }\r\n            console.log(turma);\r\n            axios.post(`${this.URL}/turmas`, turma).then(response => {\r\n                console.log(response);\r\n                this.getAllTurmas();\r\n            });\r\n            console.log('seg:' +this.seg);\r\n            console.log('ter:' +this.ter);\r\n            console.log('qua:' +this.qua);\r\n            console.log('qui:' +this.qui);\r\n            console.log('sex:' +this.sex);\r\n            console.log('sab:' +this.sab);\r\n            this.limpaDadosFormulario();\r\n        },\r\n        carregarDadosTurma(id){\r\n            this.editar = true;\r\n            axios.get(`${this.URL}/turmas/getone/`+id).then(response => {\r\n                const turma = response.data[0]\r\n                this.turmaId = turma.id_turma\r\n                this.nome = turma.nome_turma\r\n                this.instrutor = turma.id_instrutor\r\n                \r\n                console.log(response.data[0]);\r\n            })\r\n        },\r\n        salvarDados(){\r\n            const turma = {\r\n                nome_turma: this.nome,\r\n                id_instrutor: this.instrutor\r\n            }\r\n            console.log(turma);\r\n            axios.put(`${this.URL}/turmas/${this.turmaId}`, turma).then(response => {\r\n                console.log(response);\r\n                this.getAllTurmas();\r\n            });\r\n            this.limpaDadosFormulario();\r\n        },\r\n        deleteTurma(id) {\r\n            axios.delete(this.URL+\"/turmas/delete/\"+id).then(()=>{\r\n                this.getAllTurmas()\r\n            });\r\n        },\r\n        getAllInstrutoresOptions() {\r\n            axios.get(`${this.URL}/instrutores/options`).then(response => {\r\n                this.instrutores = response.data\r\n                console.log(this.instrutores);\r\n            })\r\n        },\r\n        limpaDadosFormulario(){\r\n            this.turmaId = 0;\r\n            this.nome = '';\r\n            this.instrutorId = 0;\r\n            this.instrutor = 0;\r\n            this.editar = false;\r\n            this.hrtreino = '';\r\n            this.qtdeSemana = '';\r\n            this.seg = false;\r\n            this.ter = false;\r\n            this.qua = false;\r\n            this.qui = false;\r\n            this.sex = false;\r\n            this,sab = false;\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getAllTurmas();\r\n        this.getAllInstrutoresOptions();\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n   \r\n</style>"],"mappings":"AAgIA,OAAOA,KAAI,MAAO,OAAM;AAGxB,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,WAAW,EAAC,EAAE;MACdC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,CAAC;MACVC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,CAAC;MACdC,SAAS,EAAE,CAAC;MACZC,MAAM,EAAE,KAAK;MAEbC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,GAAG,EAAE,KAAK;MACVC,GAAG,EAAE,KAAK;MACVC,GAAG,EAAE,KAAK;MACVC,GAAG,EAAE,KAAK;MACVC,GAAG,EAAE,KAAK;MACVC,GAAG,EAAE;IACT;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAG;MACXrB,KAAK,CAACsB,GAAG,CAAE,GAAE,IAAI,CAACnB,GAAI,SAAQ,CAAC,CAACoB,IAAI,CAACC,QAAO,IAAK;QAC7C,IAAI,CAACnB,MAAK,GAAImB,QAAQ,CAACtB,IAAG;QAC1BuB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACrB,MAAM,CAAC;QACxB,KAAIsB,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACtB,MAAM,CAACuB,MAAM,EAAED,CAAC,EAAE,EACtC;UACI,IAA