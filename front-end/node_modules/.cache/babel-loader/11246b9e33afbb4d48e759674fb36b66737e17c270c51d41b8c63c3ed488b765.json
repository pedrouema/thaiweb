{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AlunoAdmin',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      alunos: [],\n      planos: [],\n      turmas: []\n    };\n  },\n  methods: {\n    getAllAlunos() {\n      axios.get(`${this.URL}/alunos`).then(response => {\n        this.alunos = response.data;\n        console.log(this.alunos);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteAluno(id) {\n      axios.delete(this.URL + \"/alunos/delete/\" + id).then(() => {\n        this.getAllAlunos();\n      });\n    },\n    getAllPlanos() {\n      axios.get(`${this.URL}/planos`).then(response => {\n        this.planos = response.data;\n        console.log(\"todos os planos\" + this.planos);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    getAllTurmas() {\n      axios.get(`${this.URL}/turmas`).then(response => {\n        this.turmas = response.data;\n        console.log(this.turmas);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  mounted() {\n    this.getAllAlunos();\n  }\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,KAAI,MAAO,OAAM;AAGxB,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,YAAY,GAAG;MACXR,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,SAAQ,CAAC,CAACO,IAAI,CAACC,QAAO,IAAK;QAC7C,IAAI,CAACP,MAAK,GAAIO,QAAQ,CAACT,IAAG;QAC1BU,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,MAAM,CAAC;MAE5B,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDC,WAAW,CAACC,EAAE,EAAE;MACZjB,KAAK,CAACkB,MAAM,CAAC,IAAI,CAACf,GAAG,GAAC,iBAAiB,GAACc,EAAE,CAAC,CAACP,IAAI,CAAC,MAAI;QACjD,IAAI,CAACF,YAAY,EAAC;MACtB,CAAC;IACL,CAAC;IACDW,YAAY,GAAG;MACXnB,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACN,GAAI,SAAQ,CAAC,CAACO,IAAI,CAACC,QAAO,IAAK;QAC7C,IAAI,CAACN,MAAK,GAAIM,QAAQ,CAACT,IAAG;QAC1BU,OAAO,CAACC,GAAG,