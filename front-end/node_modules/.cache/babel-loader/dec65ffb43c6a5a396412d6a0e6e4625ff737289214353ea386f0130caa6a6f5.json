{"ast":null,"code":"export default {\n  name: 'GerenciarDespesas',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      despesas: [],\n      nome: '',\n      valor: '',\n      dataVenc: '',\n      dataIni: '',\n      dataFim: '',\n      despesaId: 0,\n      verTabela: false\n    };\n  },\n  methods: {\n    buscarDespesas() {},\n    getAllDespesas() {\n      axios.get(`${this.URL}/despesas`).then(response => {\n        this.despesas = response.data;\n        console.log(this.despesas);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    carregarDadosDespesa(id) {\n      axios.get(`${this.URL}/despesas/getone/` + id).then(response => {\n        const despesa = response.data[0];\n        this.despesaId = despesa.id_despesa;\n        this.nome = despesa.nome_despesa;\n        this.valor = despesa.valor_despesa;\n        this.dataVenc = despesa.vencimento_despesa_formatada;\n        console.log(response.data[0]);\n      });\n    }\n  },\n  mounted() {\n    this.getAllDespesas();\n  }\n};","map":{"version":3,"mappings":"AAsDA,eAAe;EACXA,IAAI,EAAE,mBAAmB;EACzBC,IAAI,GAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,CAAC;MACZC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,cAAc,GAAE,CAEhB,CAAC;IACDC,cAAc,GAAG;MACbC,KAAK,CAACC,GAAG,CAAE,GAAE,IAAI,CAACb,GAAI,WAAU,CAAC,CAACc,IAAI,CAACC,QAAO,IAAK;QAC/C,IAAI,CAACd,QAAO,GAAIc,QAAQ,CAAChB,IAAG;QAC5BiB,OAAO,CAACC,GAAG,CAAC,IAAI,CAAChB,QAAQ,CAAC;MAC9B,CAAC,EACAiB,KAAK,CAACC,KAAI,IAAK;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACtB,CAAC;IACL,CAAC;IACDC,oBAAoB,CAACC,EAAE,EAAC;MACpBT,KAAK,CAACC,GAAG,CAAE,GAAE,IAAI,CAACb,GAAI,mBAAkB,GAACqB,EAAE,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QAC1D,MAAMO,OAAM,GAAIP,QAAQ,CAAChB,IAAI,CAAC,CAAC;QAC/B,IAAI,CAACQ,SAAQ,GAAIe,OAAO,CAACC,UAAS;QAClC,IAAI,CAACrB,IAAG,GAAIoB,OAAO,CAACE,YAAW;QAC/B,IAAI,CAACrB,KAAI,GAAImB,OAAO,CAACG,aAAY;QACjC,I