{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'MensalidadesAtrasadas',\n  data() {\n    return {\n      URL: \"http://localhost:4000\",\n      alunos: [],\n      atrasado: [],\n      pago: [],\n      atrasados: [],\n      nome: '',\n      mesAtual: '',\n      diaAtual: '',\n      anoAtual: '',\n      dataAtual: '',\n      mesAno: ''\n    };\n  },\n  methods: {\n    //RETORNAR TODOS ALUNOS TIPO PLANO MENSALIDADE\n    getAllAlunosMensalidade() {\n      axios.get(`${this.URL}/alunosmensalidade`).then(response => {\n        this.alunos = response.data;\n        this.getAllJaPagouNoMes();\n      });\n    },\n    //FUNÇÃO TESTE\n    getAllJaPagouNoMes() {\n      this.pegarDataAtual();\n      axios.get(`${this.URL}/recebimentoatrasadas/` + this.mesAno).then(response => {\n        this.pago = response.data;\n      });\n      console.log(this.pago + '<<<<<<<<<<');\n      this.listaEmAtrasos();\n    },\n    listaEmAtrasos() {\n      for (let i = 0; i < this.pago.length; i++) {\n        for (let j = 0; this.alunos.length; j++) {\n          if (this.pago[i].id_aluno == this.alunos[j].id_aluno) {\n            this.alunos.splice(j, 1);\n          }\n        }\n      }\n      console.log(this.alunos);\n    },\n    pegarDataAtual() {\n      const data = new Date();\n      const dia = String(data.getDate()).padStart(2, '0');\n      const mes = String(data.getMonth() + 1).padStart(2, '0');\n      const ano = String(data.getFullYear());\n      const atual = `${dia}/${mes}/${ano}`;\n      this.mesAtual = mes;\n      this.diaAtual = dia;\n      this.anoAtual = ano;\n      this.dataAtual = atual;\n      this.mesAno = mes + '-' + ano;\n    },\n    limpaDados() {\n      this.atrasado = '';\n    }\n  },\n  mounted() {\n    this.getAllAlunosMensalidade();\n  }\n};","map":{"version":3,"names":["axios","name","data","URL","alunos","atrasado","pago","atrasados","nome","mesAtual","diaAtual","anoAtual","dataAtual","mesAno","methods","getAllAlunosMensalidade","get","then","response","getAllJaPagouNoMes","pegarDataAtual","console","log","listaEmAtrasos","i","length","j","id_aluno","splice","Date","dia","String","getDate","padStart","mes","getMonth","ano","getFullYear","atual","limpaDados","mounted"],"sources":["C:\\Users\\Pedro Uema\\Desktop\\thaiweb\\front-end\\src\\components\\financeiro\\mensalidadesAtrasadas.vue"],"sourcesContent":["<template>\r\n    <h3>Mensalidades em atraso</h3>\r\n    <form class=\"form-inline\">\r\n        <h5>Busque pelo o nome do aluno que deseja verificar!</h5>\r\n        <div class=\"form-group mb-2\">\r\n            <label for=\"inputZip\" class=\"form-label\">Digite o nome do aluno</label >\r\n            <input type=\"text\" class=\"form-control\" v-model=\"nome\">\r\n        </div>\r\n        <button type=\"button\" class=\"btn btn-primary\" @click=\"buscar()\">\r\n            <i class=\"fa fa-search\"></i> BUSCAR</button>\r\n    </form>\r\n    <br>\r\n    <div class=\"alunos-table\">\r\n        <table class=\"table\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Nome</th>\r\n                    <th scope=\"col\">CPF</th>\r\n                    <th scope=\"col\">Plano</th>\r\n                    <th scope=\"col\">Valor Plano</th>\r\n                    <th scope=\"col\">Dia Vencimento</th>\r\n                    <th id=\"mesAtraso\" scope=\"col\">Mês em Atraso</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n                <tr v-for=\"receb in atrasados\" :key=\"receb.id_mensalidade\">\r\n                    <th scope=\"row\">{{ receb.nome_aluno }}</th>\r\n                    <td>{{ receb.cpf_aluno }}</td>\r\n                    <td>{{ receb.nome_plano }}</td>\r\n                    <td>R$ {{ receb.valor_plano.toFixed(2) }}</td>\r\n                    <td>{{ receb.diapag_format }}</td>\r\n                    <td>{{ this.mesAtual }}</td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'MensalidadesAtrasadas',\r\n    data(){\r\n        return {\r\n            URL: \"http://localhost:4000\",\r\n            alunos: [],\r\n            atrasado: [],\r\n            pago: [],\r\n            atrasados: [],\r\n            nome: '',\r\n            mesAtual: '',\r\n            diaAtual: '',\r\n            anoAtual: '',\r\n            dataAtual: '',\r\n            mesAno: '',\r\n        }\r\n    },\r\n    methods: {\r\n        //RETORNAR TODOS ALUNOS TIPO PLANO MENSALIDADE\r\n        getAllAlunosMensalidade() {\r\n            axios.get(`${this.URL}/alunosmensalidade`).then(response => {\r\n                this.alunos = response.data\r\n                this.getAllJaPagouNoMes();\r\n            })\r\n        },\r\n        //FUNÇÃO TESTE\r\n        getAllJaPagouNoMes() {\r\n            this.pegarDataAtual();\r\n            axios.get(`${this.URL}/recebimentoatrasadas/`+this.mesAno).then(response => {\r\n                this.pago = response.data\r\n            })\r\n            console.log(this.pago+ '<<<<<<<<<<');\r\n            this.listaEmAtrasos();\r\n        },\r\n        listaEmAtrasos(){\r\n            for(let i = 0; i < this.pago.length; i++)\r\n            {\r\n                for(let j = 0; this.alunos.length; j++)\r\n                {\r\n                    if(this.pago[i].id_aluno == this.alunos[j].id_aluno)\r\n                    {\r\n                        this.alunos.splice(j, 1)\r\n                    }\r\n                }\r\n            }\r\n            console.log(this.alunos);\r\n        },\r\n        pegarDataAtual() {\r\n            const data = new Date()\r\n            const dia = String(data.getDate()).padStart(2, '0')\r\n            const mes = String(data.getMonth() + 1).padStart(2, '0')\r\n            const ano = String(data.getFullYear())\r\n            const atual = `${dia}/${mes}/${ano}`\r\n            this.mesAtual = mes\r\n            this.diaAtual = dia\r\n            this.anoAtual = ano\r\n            this.dataAtual = atual\r\n            this.mesAno = mes+'-'+ano\r\n\r\n        },\r\n        limpaDados() {\r\n            this.atrasado = '';\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getAllAlunosMensalidade();\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    th#mesAtraso{\r\n        background-color: rgb(192, 56, 56);\r\n    }\r\n</style>"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,uBAAuB;EAC7BC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,GAAG,EAAE,uBAAuB;MAC5BC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL;IACAC,uBAAuBA,CAAA,EAAG;MACtBf,KAAK,CAACgB,GAAG,CAAE,GAAE,IAAI,CAACb,GAAI,oBAAmB,CAAC,CAACc,IAAI,CAACC,QAAO,IAAK;QACxD,IAAI,CAACd,MAAK,GAAIc,QAAQ,CAAChB,IAAG;QAC1B,IAAI,CAACiB,kBAAkB,EAAE;MAC7B,CAAC;IACL,CAAC;IACD;IACAA,kBAAkBA,CAAA,EAAG;MACjB,IAAI,CAACC,cAAc,EAAE;MACrBpB,KAAK,CAACgB,GAAG,CAAE,GAAE,IAAI,CAACb,GAAI,wBAAuB,GAAC,IAAI,CAACU,MAAM,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QACxE,IAAI,CAACZ,IAAG,GAAIY,QAAQ,CAAChB,IAAG;MAC5B,CAAC;MACDmB,OAAO,CAACC,GAAG,CAAC,IAAI,CAAChB,IAAI,GAAE,YAAY,CAAC;MACpC,IAAI,CAACiB,cAAc,EAAE;IACzB,CAAC;IACDA,cAAcA,CAAA,EAAE;MACZ,KAAI,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAAClB,IAAI,CAACmB,MAAM,EAAED,CAAC,EAAE,EACxC;QACI,KAAI,IAAIE,CAAA,GAAI,CAAC,EAAE,IAAI,CAACtB,MAAM,CAACqB,MAAM,EAAEC,CAAC,EAAE,EACtC;UACI,IAAG,IAAI,CAACpB,IAAI,CAACkB,CAAC,CAAC,CAACG,QAAO,IAAK,IAAI,CAACvB,MAAM,CAACsB,CAAC,CAAC,CAACC,QAAQ,EACnD;YACI,IAAI,CAACvB,MAAM,CAACwB,MAAM,CAACF,CAAC,EAAE,CAAC;UAC3B;QACJ;MACJ;MACAL,OAAO,CAACC,GAAG,CAAC,IAAI,CAAClB,MAAM,CAAC;IAC5B,CAAC;IACDgB,cAAcA,CAAA,EAAG;MACb,MAAMlB,IAAG,GAAI,IAAI2B,IAAI,EAAC;MACtB,MAAMC,GAAE,GAAIC,MAAM,CAAC7B,IAAI,CAAC8B,OAAO,EAAE,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG;MAClD,MAAMC,GAAE,GAAIH,MAAM,CAAC7B,IAA