    console.log(instrutor);\r\n            axios.post(`${this.URL}/planos`, plano).then(response => {\r\n                console.log(response);\r\n                this.getAllPlanos();\r\n            })\r\n            this.limpaDadosFormulario()\r\n        },\r\n        deletePlano(id) {\r\n            axios.delete(this.URL+\"/planos/delete/\"+id).then(()=>{\r\n                this.getAllPlanos()\r\n            })\r\n        },\r\n        limpaDadosFormulario(){\r\n            this.planoId = 0;\r\n            this.nome = '';\r\n            this.valor = 0;\r\n            this.editar = false;\r\n        },\r\n    },\r\n    mounted() {\r\n        this.getAllPlanos();\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}